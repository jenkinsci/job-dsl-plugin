          </includes>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <executions>
          <execution>
            <id>download-update-center</id>
            <goals>
              <goal>run</goal>
            </goals>
            <phase>generate-sources</phase>
            <configuration>
              <target>
                <get dest="${project.build.directory}/update-center.jsonp" src="https://updates.jenkins.io/update-center.json" />
                <copy file="${project.build.directory}/update-center.jsonp" tofile="${project.build.directory}/update-center.json">
                  <filterchain>
                    <tailfilter lines="2" />
                    <headfilter lines="1" />
                  </filterchain>
                </copy>
              </target>
            </configuration>
          </execution>
          <execution>
            <goals>
              <goal>run</goal>
            </goals>
            <phase>verify</phase>
            <configuration>
              <target>
                <taskdef classname="org.codenarc.ant.CodeNarcTask" name="codenarc" />
                <echo level="info" message="Checking src/main/groovy code with CodeNarc" />
                <codenarc failOnError="true" maxPriority1Violations="0" maxPriority2Violations="0" maxPriority3Violations="0" ruleSetFiles="file:${maven.multiModuleProjectDirectory}/config/codenarc/rules.groovy">
                  <fileset dir="src/main/groovy">
                    <include name="**/*.groovy" />
                  </fileset>
                  <report type="console" />
                </codenarc>
                <echo level="info" message="Checking src/test/groovy code with CodeNarc" />
                <codenarc failOnError="true" maxPriority1Violations="0" maxPriority2Violations="0" maxPriority3Violations="0" ruleSetFiles="file:${maven.multiModuleProjectDirectory}/config/codenarc/rules-test.groovy">
                  <fileset dir="src/test/groovy">
                    <include name="**/*.groovy" />
                  </fileset>
                  <report type="console" />
                </codenarc>
              </target>
            </configuration>
          </execution>
        </executions>
